@model IEnumerable<Health_Insurance.Models.ContactU>
@{
    ViewData["Title"] = "Index";
    Layout = "AdminLayout";

}



 

<!-- Table Start -->
<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-secondary rounded h-100 p-4">

                <div class="table-responsive">
                    <h1>Contact Us</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>Address</th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>massege
            </th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddressText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Text1)
             </td>
             <td>@ViewBag.CurrentDate</td>
            <td>
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
         </tr>
}
    </tbody>
    <tfoot>
        <tr>
             <th>number of Countact Us</th>
             <th>@ViewBag.CountCountactUs</th>
        </tr>
    </tfoot>
</table>
                </div>
            </div>
        </div>
    </div>
</div> 
  